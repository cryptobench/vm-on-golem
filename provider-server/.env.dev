GOLEM_PROVIDER_ENVIRONMENT="development"
# Debug mode
GOLEM_PROVIDER_DEBUG=true

# Server settings
GOLEM_PROVIDER_HOST=0.0.0.0
GOLEM_PROVIDER_PORT=7466

# Provider identity
GOLEM_PROVIDER_ID=provider123
GOLEM_PROVIDER_NAME=golem-provider-dev
GOLEM_PROVIDER_COUNTRY=SE

# Discovery service
GOLEM_PROVIDER_DISCOVERY_URL=http://195.201.39.101:9001
GOLEM_PROVIDER_ADVERTISEMENT_INTERVAL=240

# VM settings
GOLEM_PROVIDER_MAX_VMS=10
